{
  "name": "cobalt spectrum",
  "colors": {
    "editor.background": "#2b2d42",
    "editor.foreground": "#cfd8dc",
    "activityBarBadge.background": "#8d99ae",
    "sideBarTitle.foreground": "#b0bec5",
    "statusBar.background": "#344666",
    "sideBar.background": "#262d3a",
    "menu.background": "#253144",
    "panel.background": "#2b374c"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#535d67"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#00eaff"
      }
    },
    {
      "name": "Function Names",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#72ffce",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#949eff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#92fffb"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#00a3fb"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#118ab2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#06d6a0"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#41949f"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.type"],
      "settings": {
        "foreground": "#9dbbd6"
      }
    },
    {
      "name": "Types",
      "scope": ["support.class"],
      "settings": {
        "foreground": "#8ac926"
      }
    },
    {
      "name": "Errors",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#bc5090",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "string.unquoted.label.json"
      ],
      "settings": {
        "foreground": "#7fbc41",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag", "meta.tag"],
      "settings": {
        "foreground": "#6fbbfe",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Components",
      "scope": ["support.class.component"],
      "settings": {
        "foreground": "#99c9ff"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name", "meta.tag.attributes"],
      "settings": {
        "foreground": "#0a9396"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#8a93be"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#06d6a0"
      }
    },
    {
      "name": "RegExp",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#8338ec",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#8d99ae"
      }
    },
    {
      "name": "Markup Headings",
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "foreground": "#17becf",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff006e"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2ec4b6"
      }
    },
    {
      "name": "Markup Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#118ab2"
      }
    },
    {
      "name": "Markup Quote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#9d4edd"
      }
    },
    {
      "name": "Markup Lists",
      "scope": [
        "markup.list",
        "markup.list.numbered",
        "markup.list.unnumbered"
      ],
      "settings": {
        "foreground": "#0077b6"
      }
    },
    {
      "name": "XML Processing",
      "scope": ["meta.preprocessor.xml"],
      "settings": {
        "foreground": "#7209b7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Diff Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#4cc9f0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#dd99a6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Diff Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#3a0ca3",
        "fontStyle": "italic"
      }
    }
  ]
}
